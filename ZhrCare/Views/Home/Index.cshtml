@model IEnumerable<ZhrCare.Models.Patient>
@using Microsoft.AspNetCore.Identity
@inject UserManager<ZhrCare.Models.ApplicationUser> UserManager
@{
    ViewData["Title"] = "Dashboard";
    var selectedId = ViewBag.SelectedPatientId;
    
    var currentUser = await UserManager.GetUserAsync(User);
    var nameToShow = currentUser?.FullName ?? User.Identity.Name;
}

<style>
    /* تعريف المتغيرات في جذر المستند لتجنب تنبيهات التحذير وضمان الشمولية */
    :root {
        --zhr-purple: #6c5ce7;
        --zhr-purple-light: #f1f0ff;
        --zhr-purple-hover: #5b4bc4;
        --zhr-text-dark: #2d3436;
        --zhr-gray: #f8f9fa;
    }

    /* الكلاس الرئيسي للخلفية ومنطقة الأمان */
    .dashboard-bg {
        background-color: var(--zhr-gray);
        min-height: 100vh;
        padding-bottom: 100px; /* حل مشكلة الفوتر: مساحة أمان سفلية */
        margin: -1rem -1rem 0 -1rem; /* إلغاء هوامش الحاوية الافتراضية لتملأ الشاشة */
    }

    .kpi-card {
        border-radius: 24px;
        border: none;
        transition: transform 0.3s ease, box-shadow 0.3s ease;
        background: #fff;
        box-shadow: 0 10px 30px rgba(0,0,0,0.03);
    }

    .kpi-card:hover {
        transform: translateY(-5px);
        box-shadow: 0 15px 35px rgba(108, 92, 231, 0.1);
    }

    .action-btn-circle {
        width: 38px;
        height: 38px;
        display: flex;
        align-items: center;
        justify-content: center;
        border-radius: 12px;
        background-color: var(--zhr-purple-light);
        color: var(--zhr-purple) !important;
        text-decoration: none;
        transition: 0.2s;
    }

    .action-btn-circle:hover {
        background-color: var(--zhr-purple);
        color: #fff !important;
    }

    .zhr-scroll {
        max-height: 350px;
        overflow-y: auto;
        padding-right: 8px;
    }

    .zhr-scroll::-webkit-scrollbar { width: 5px; }
    .zhr-scroll::-webkit-scrollbar-thumb { background: #e2e2e2; border-radius: 10px; }

    .list-item-custom {
        border: 1px solid #f1f1f1;
        border-radius: 16px;
        padding: 12px;
        margin-bottom: 10px;
        transition: 0.2s;
    }

    .list-item-custom:hover {
        border-color: var(--zhr-purple-light);
        background-color: #fcfcff;
    }

    .sidebar-gradient {
        background: linear-gradient(145deg, var(--zhr-purple), #8e7dff);
        border-radius: 24px;
        border: none;
    }
</style>

<div class="dashboard-bg"> <div class="container-fluid pt-4 px-4">
        
        <div class="row mb-5 align-items-end">
            <div class="col-lg-8 text-center text-lg-start mb-4 mb-lg-0">
                <h1 class="fw-bold" style="color: var(--zhr-text-dark);">
                    Dashboard, <span style="color: var(--zhr-purple);">@nameToShow</span> 👋
                </h1>
                <p class="text-muted mb-0">Monitor care activities and patient health at a glance.</p>
            </div>
            <div class="col-lg-4">
                <div class="card border-0 shadow-sm p-3" style="border-radius: 18px;">
                    <form asp-action="Index" method="get" id="pSwitchForm">
                        <label class="small text-muted fw-bold mb-1 ms-1 text-uppercase">Selected Patient View</label>
                        <select name="selectedPatientId" onchange="this.form.submit()" class="form-select border-0 bg-light fw-bold" style="color: var(--zhr-purple); border-radius: 10px;">
                            <option value="">Full System Overview</option>
                            @if (ViewBag.AllPatients != null) {
                                foreach (var p in ViewBag.AllPatients) {
                                    <option value="@p.Id" selected="@(selectedId == p.Id)">@p.Name</option>
                                }
                            }
                        </select>
                    </form>
                </div>
            </div>
        </div>

        <div class="row g-4 mb-5">
            <div class="col-md-4">
                <div class="card kpi-card p-4">
                    <div class="d-flex align-items-center">
                        <div class="rounded-4 p-3 me-3" style="background: var(--zhr-purple-light); color: var(--zhr-purple);">
                            <i class="bi bi-people fs-3"></i>
                        </div>
                        <div>
                            <span class="text-muted small fw-bold">Active Patients</span>
                            <h2 class="fw-bold mb-0">@(ViewBag.TotalPatients ?? 0)</h2>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="card kpi-card p-4">
                    <div class="d-flex align-items-center">
                        <div class="rounded-4 p-3 me-3" style="background: #fff9e6; color: #d6a01d;">
                            <i class="bi bi-capsule fs-3"></i>
                        </div>
                        <div>
                            <span class="text-muted small fw-bold">Today's Meds</span>
                            <h2 class="fw-bold mb-0">@(ViewBag.TodayMedsCount ?? 0)</h2>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="card kpi-card p-4">
                    <div class="d-flex align-items-center"> 
                        <div class="rounded-4 p-3 me-3" style="background: #e6fffe; color: #00cec9;">
                            <i class="bi bi-camera-reels fs-3"></i>
                        </div>
                        <div>
                            <span class="text-muted small fw-bold">Memory Logs</span>
                            <h2 class="fw-bold mb-0">@(ViewBag.TotalMemories ?? 0)</h2>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="row g-4">
            <div class="col-lg-8">
                <div class="card kpi-card p-4 mb-4">
                    <div class="d-flex justify-content-between align-items-center mb-4">
                        <h5 class="fw-bold mb-0 text-dark"><i class="bi bi-heart-pulse me-2" style="color: var(--zhr-purple);"></i>Recent Care Profiles</h5>
                        <a asp-controller="Patients" asp-action="Index" class="btn btn-sm fw-bold" style="color: var(--zhr-purple); background: var(--zhr-purple-light); border-radius: 10px;">View All</a>
                    </div>
                    <div class="table-responsive">
                        <table class="table table-hover align-middle border-0">
                            <thead class="text-muted small text-uppercase">
                                <tr><th>Name</th><th>Age</th><th class="text-end">Quick Access</th></tr>
                            </thead>
                            <tbody>
                                @if (Model != null) {
                                    foreach (var p in Model) {
                                        <tr>
                                            <td><span class="fw-bold text-dark">@p.Name</span></td>
                                            <td><span class="badge bg-light text-dark px-3 py-2 rounded-pill">@p.Age Years</span></td>
                                            <td>
                                                <div class="d-flex gap-2 justify-content-end">
                                                    <a asp-controller="Medications" asp-action="Index" asp-route-patientId="@p.Id" class="action-btn-circle" title="Meds"><i class="bi bi-capsule"></i></a>
                                                    <a asp-controller="MemoryRecords" asp-action="Index" asp-route-patientId="@p.Id" class="action-btn-circle" title="Memories"><i class="bi bi-images"></i></a>
                                                    <a asp-controller="Routines" asp-action="Index" asp-route-patientId="@p.Id" class="action-btn-circle" title="Routines"><i class="bi bi-calendar-check"></i></a>
                                                </div>
                                            </td>
                                        </tr>
                                    }
                                }
                            </tbody>
                        </table>
                    </div>
                </div>

                <div class="row g-4">
                    <div class="col-md-6">
                        <div class="card kpi-card p-4">
                            <h6 class="fw-bold mb-4" style="color: #d6a01d;"><i class="bi bi-clock-history me-2"></i>Medical Timeline</h6>
                            <div class="zhr-scroll">
                                @if (ViewBag.TodayMedications != null) {
                                    foreach (var med in ViewBag.TodayMedications) {
                                        <div class="list-item-custom d-flex align-items-center">
                                            <div class="badge bg-white border text-dark me-3 shadow-sm p-2" style="min-width: 60px;">@med.ScheduledTime.ToString(@"hh\:mm")</div>
                                            <div>
                                                <div class="fw-bold small text-dark">@med.Name</div>
                                                <div class="text-muted" style="font-size: 0.75rem;"><i class="bi bi-person me-1"></i>@med.Patient?.Name</div>
                                            </div>
                                        </div>
                                    }
                                }
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="card kpi-card p-4">
                            <h6 class="fw-bold mb-4 text-success"><i class="bi bi-calendar-check me-2"></i>Daily Routine</h6>
                            <div class="zhr-scroll">
                                @if (ViewBag.TodayRoutines != null) {
                                    foreach (var routine in ViewBag.TodayRoutines) {
                                        <div class="list-item-custom d-flex align-items-center">
                                            <div class="badge bg-white border text-dark me-3 shadow-sm p-2" style="min-width: 60px;">@routine.Time.ToString(@"hh\:mm")</div>
                                            <div>
                                                <div class="fw-bold small text-dark">@routine.ActivityName</div>
                                                <div class="text-muted" style="font-size: 0.75rem;"><i class="bi bi-person me-1"></i>@routine.Patient?.Name</div>
                                            </div>
                                        </div>
                                    }
                                }
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="col-lg-4">
                <div class="card sidebar-gradient p-4 text-white mb-4 shadow-lg">
                    <h5 class="fw-bold mb-4"><i class="bi bi-lightning-charge-fill me-2"></i>Quick Manager</h5>
                    @if (selectedId != null) {
                        <p class="small opacity-75 mb-3">Adding data for: <strong>@ViewBag.SelectedPatientName</strong></p>
                        <div class="d-grid gap-3 mb-4">
                            <a asp-controller="Medications" asp-action="Create" asp-route-patientId="@selectedId" class="btn btn-light fw-bold py-2 border-0 shadow-sm" style="color: #d6a01d; border-radius: 12px;">+ New Medication</a>
                            <a asp-controller="Routines" asp-action="Create" asp-route-patientId="@selectedId" class="btn btn-light fw-bold py-2 border-0 shadow-sm" style="color: #27ae60; border-radius: 12px;">+ New Routine</a>
                            <a asp-controller="MemoryRecords" asp-action="Create" asp-route-patientId="@selectedId" class="btn btn-light fw-bold py-2 border-0 shadow-sm" style="color: #00cec9; border-radius: 12px;">+ New Memory Log</a>
                        </div>
                        <hr class="opacity-25 my-4">
                    }
                    <div class="d-grid">
                        <a asp-controller="Patients" asp-action="Create" class="btn btn-dark fw-bold py-3 shadow border-0" style="background: rgba(0,0,0,0.25); border-radius: 14px;">
                            <i class="bi bi-person-plus-fill me-2"></i>Register New Patient
                        </a>
                    </div>
                </div>
                
                <div class="card kpi-card p-4 text-center border-0 shadow-sm">
                    <div class="mb-3">
                        <i class="bi bi-heart-fill text-danger fs-1"></i>
                    </div>
                    <h6 class="fw-bold text-dark">Caregiver Quote</h6>
                    <p class="fst-italic text-muted small px-2">"Caregiving is an act of love, spirit, and soul."</p>
                </div>
            </div>
        </div>
    </div>
</div>